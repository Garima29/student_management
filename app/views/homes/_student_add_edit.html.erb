<div class="container hidden" id="addEditStudentForm">
 <input type="hidden" id="student_id" value="">
 <h2>Add/edit student</h2>
 <form class="form-horizontal" action="" method="" id="studentForm">
   <div class="form-group">
     <label class="col-lg-2 control-label">Name</label>
     <div class="col-lg-6">
       <input type="text" name="student_name" id="studentName" class="form-control" value=""/>
     </div>
   </div>
   <div class="form-group">
     <label class="col-lg-2 control-label">Father's name</label>
     <div class="col-lg-6">
       <input type="text" name="student_father_name" id="studentFatherName" class="form-control" value=""/>
     </div>
   </div>
   <div class="form-group">
     <label class="col-lg-2 control-label">Mother's name</label>
     <div class="col-lg-6">
       <input type="text" name="student_mother_name" id="studentMotherName" class="form-control" value=""/>
     </div>
   </div>
   <div class="form-group">
     <label class="col-lg-2 control-label">City</label>
     <div class="col-lg-6">
       <input type="text" name="student_city_name" id="studentCity" class="form-control" value=""/>
     </div>
   </div>
   <div class="form-group">
     <label class="col-lg-2 control-label">State</label>
     <div class="col-lg-6">
      <input type="text" name="student_state_name" id="studentState" class="form-control" value=""/>
     </div>
   </div>
   <div class="form-group">
     <label class="col-lg-2 control-label">Phone no</label>
     <div class="col-lg-6">
       <input type="text" name="student_phoneno" id="studentPhoneno" class="form-control" value=""/>
     </div>
   </div>
   <div class="form-group">
     <label class="col-lg-2 control-label">Classroom</label>
       <div class="col-lg-6">
         <select name="student_classroom_list_element" id="studentClassroomListElement" class="form-control" >
         </select>
     </div>
   </div>
   <button type="submit" class="btn btn-primary" id="studentAdd">Add</button>
   <button type="submit" class="btn btn-primary hidden" id="studentUpdate">Edit</button>
 </form>
  <div class="sideIcons">
    <div id="backLinkGoHome" >
      <a class="back-link-title"><img src="assets/home.png" alt="img"></a>
    </div>
    <div id="backLinkGoBack">
      <a class="back-link-title"><img src="assets/back-arrow.png" alt="img"></a>
    </div>
  </div>
</div>
